<web-app>
  <display-name>SomethingNew</display-name>
  <servlet>
    <servlet-name>HelloServerlet</servlet-name>
    <servlet-class>com.tannv.controllers.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServerlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
</web-app>
